    "version": "0.2.0",
    "configurations": [
        {
            "name": "editcli",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/edit_cli.py",
            "args": [
                "--input",
                "../textual_inversion/vases/vase4.png",
                "--output",
                "../instructedits/",
                "--edit",
                "paint the statue purple"],
                //"--percent",
                //"0.01"],
            "env": {
              "CUDA_VISIBLE_DEVICES": "0",
            },
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "minimal",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/minimal.py",
            "args": [
                "--base",
                "configs/latent-diffusion/txt2img-1p4B-finetune.yaml",
                "-t",
                "--actual_resume",
                "models/ldm/text2img-large/model.ckpt",
                "-n",
                "run",
                "--gpus",
                "0,",
                "--data_root",
                "vases/",
                "--init_word",
                "green"],
                //"--percent",
                //"0.01"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "train_dbg",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--base",
                "configs/latent-diffusion/txt2img-1p4B-finetune.yaml",
                "-t",
                "--actual_resume",
                "models/ldm/text2img-large/model.ckpt",
                "-n",
                "run",
                "--gpus",
                "0,",
                "--data_root",
                "vases/",
                "--init_word",
                "vase"],
                //"--percent",
                //"0.01"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "train_instructpix2pix",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--base",
                "configs/latent-diffusion/instructpix2pix.yaml",
                "-t",
                "--actual_resume",
                "models/ldm/stable-diffusion-v1/instruct-pix2pix-00-22000.ckpt",
                "-n",
                "run",
                "--gpus",
                "0,",
                "--data_root",
                "vases/",
                "--init_word",
                "vase"],
                //"--percent",
                //"0.01"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "inference",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/musclesinaction/viz_test.py",
            "args": [
                "--name",
                "dbg",
                "--bs",
                "1"],
            "env": {
              "CUDA_VISIBLE_DEVICES": "1",
            },
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "edit",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/musclesinaction/editemg.py",
            "args": [
                "--name",
                "dbg",
                "--bs",
                "1"],
            "env": {
              "CUDA_VISIBLE_DEVICES": "1",
            },
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
        },
    ]
}
